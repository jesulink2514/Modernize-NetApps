{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "LegacyDb": "Data Source=.\\SQLEXPRESS;Initial Catalog=Legacy;Integrated Security=SSPI;"
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "catchall": {
        "ClusterId": "legacy",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "legacy": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:5000/"
          }
        }
      }
    }
  }
}
